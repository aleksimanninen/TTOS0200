<Window x:Class="VKO46AutoDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VKO46AutoDemo"
        mc:Ignorable="d"
        Title="Esa`s CarGarage" Height="350" Width="525">
        
    <Grid>
        <!--Ulompi StackPanel, elementit päällekkäin pinossa-->
        <StackPanel>
            <StackPanel Orientation="Horizontal">
                <Button x:Name="btnHaeAutot" Content="Hae Autot" Margin="5" Click="btnHaeAutot_Click"/>
                <Button x:Name="btnHaeAudit" Content="Hae Audit" Margin="5" Click="btnHaeAudit_Click"/>
                <ComboBox x:Name="cmdAutot" Margin="5" SelectionChanged="cmdAutot_SelectionChanged"/>
            </StackPanel>
            <DataGrid x:Name="dgAutot" SelectionChanged="dgAutot_SelectionChanged"></DataGrid>
            <Image x:Name="imgAutot"></Image>
        </StackPanel>
    </Grid>
</Window>
